// Font
@font-face {
  font-family: "CalibreMedium";
  font-display: fallback;
  src: url("./Calibre-Medium.woff") format("woff");
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: "CalibreRegular";
  font-display: fallback;
  src: url("./Calibre-Regular.woff") format("opentype");
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: "CalibreThin";
  font-display: fallback;
  src: url("./Calibre-Thin.woff") format("woff");
  font-weight: normal;
  font-style: normal;
}

$font-header: "CalibreMedium", sans-serif;
$font-body: "CalibreRegular", sans-serif;
$font-text: "CalibreRegular", sans-serif;
// Colors
$color-white: rgba(255, 255, 255, 1);
$color-dark-grey: rgba(215, 215, 215, 1);
$color-light-grey: rgba(246, 246, 246, 1);
$color-black: rgba(0, 0, 0, 1);
$color-light-green: rgba(173, 218, 27, 1);
$color-text-grey: rgba(128, 130, 132, 1);
$color-hover-grey: rgba(76, 76, 75, 1);
$color-green: rgba(92, 194, 85, 1);
$color-darker-grey: #4c4c4b;
// Container
$container-padding-phone: 4%;
$container-padding-tablet: 3%;
$container-padding-desktop: 3%;

// General
html,
body {
  padding: 0;
  margin: 0;
}

* {
  box-sizing: border-box;
}

html {
  font-size: 62.5%;
}

body {
  background-color: $color-white;
  font-family: $font-body;
  font-size: 1.4rem;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  letter-spacing: 1px;
}
body.locked {
  overflow: hidden;
}
.center-flex {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.flex-column {
  display: flex;
  flex-direction: column;
}
.small-padding {
  @media (max-width: 480px) {
    padding: 0 $container-padding-tablet !important;
  }
}
// Typography
h1,
h2,
h3,
h5,
h6 {
  font-family: $font-body;
  display: inline-block;
  margin: 0 0 10px;
  width: 100%;
  letter-spacing: 1px;
}

h1 {
  font-size: 5.2rem;

  @media (max-width: 480px) {
    font-size: 3rem;
  }
}

h2 {
  font-size: 4.2rem;
  font-family: $font-body;
  font-weight: normal;
}
h4 {
  font-family: $font-body;
  font-size: 1.4rem;
  font-weight: normal;
  letter-spacing: 1px;
}
p {
  font-family: $font-text;
  font-size: 1.6rem;
  line-height: 1.25;
  margin: 0 0 10px;
}

// Links
a,
a:active,
a:hover,
a:visited {
  text-decoration: none;
}

img,
picture {
  width: 100%;
  max-width: 100%;
  display: inline-block;
}

// Layout
.main {
}

.container {
  position: relative;
  max-width: 2000px;
  margin: 0 auto;
  width: 100%;
  padding: 0 $container-padding-phone;

  @media (min-width: 768px) {
    padding: 0 $container-padding-tablet;
  }
  @media (min-width: 1024px) {
    padding: 0 $container-padding-desktop;
  }
}
.container-padding {
  padding: 40px 0;

  @media (max-width: 480px) {
    padding: 60px 0;
  }
}
.container-padding-large {
  padding: 80px 0;

  @media (max-width: 480px) {
    padding: 60px 0;
  }
}
.container-full {
  max-width: 100%;
}

.section {
  position: relative;
  display: block;
  width: 100%;
}

.page {
}

.page_title {
  color: $color-darker-grey;
  font-weight: normal;
  font-size: 3rem;
  padding: 80px 0 0 0;
}
.page_header {
  margin-bottom: 50px;

  & a {
    color: $color-darker-grey;
  }
}

// Helpers
.text-left {
  text-align: left;
}
.text-center {
  text-align: center;
}
.text-right {
  text-align: right;
}

.hidden {
  display: none;
}

.image {
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  height: auto;
}

// Button
.button {
  font-family: $font-body;
  color: $color-black;
  background: $color-white;
  display: inline-block;
  font-size: 1.6rem;
  padding: 25px;
  height: 40px;
  line-height: 0px;
  min-width: 100px;
  text-align: center;
  text-transform: capitalize;
  position: relative;
  outline: none;
  -webkit-appearance: none;
  cursor: pointer;
  letter-spacing: 1px;

  &:disabled {
    cursor: not-allowed;
    opacity: 0.85;
  }
}
button:hover {
  color: $color-white;
  background: $color-hover-grey;
}
.button:hover {
  color: $color-white;
  background: $color-hover-grey;
}
.button-black {
  color: $color-white;
  background: #4c4c4b;
  text-align: center;
  width: 100%;
  border: none;

  &:hover {
    background: $color-black;
  }
}
.button-small {
  padding: 15px;
  min-width: auto;
  font-size: inherit;
  height: 20px;
}
.button-white {
  color: $color-black;
  background: $color-white;
  text-align: center;
  width: 100%;
  border: none;

  &:hover {
    background: $color-black;
  }
}
.button-grey {
  color: $color-white;
  background: #c3c3c3;
  text-align: center;
  width: 100%;
  border: none;
  margin-bottom: 10px;
}
.button-transparent {
  color: $color-black;
  background: transparent;
  margin: 20px 0;
  width: 100%;
  border: 0.2px solid #e3e3e3;

  &:hover {
    border-color: $color-black;
  }
}
.button-round {
  border-radius: 10px;
}
.button-close {
  background-image: url("https://cdn.shopify.com/s/files/1/0274/8609/3447/files/trash.png?v=1604585132");
  height: 15px;
  width: 15px;
  border: none;
  background-position: 50% 0;
  background-size: contain;
  background-repeat: no-repeat;
  background-color: transparent;
  cursor: pointer;

  &:hover {
    background-image: url("https://cdn.shopify.com/s/files/1/0274/8609/3447/files/trash.png?v=1604585132");
    height: 15px;
    width: 15px;
    border: none;
    background-position: 50% 0;
    background-size: contain;
    background-repeat: no-repeat;
    background-color: transparent;
  }
}
.button-blue {
  background: #1c62cf;
  color: $color-white;
  text-align: center;
  width: 100%;
  border: none;
}
.button-blue:hover {
  background: #0e4291;
}
.button-cart {
  font-weight: bold;
  height: auto;
  line-height: 16px;
}
/*------------ Customer ------------*/
.customer_name {
  color: $color-darker-grey;
  font-weight: normal;
  font-size: 3rem;
  padding: 80px 0 0 0;
}
// Input
.form {
  width: 100%;
}
.form-small {
  max-width: 350px;
  margin: 0 auto;
}
.form-margin {
  margin-bottom: 80px !important;
}
.contactForm {
  border-top: 0.2px solid #e3e3e3;
  padding-top: 40px;
  text-align: left;
}
.contactForm-white {
  border: none;
  padding: 0;
  text-align: left;

  & h3 {
    font-size: 1.8rem;
    font-family: $font-body;
    margin: 20px 0;
  }
}

.formMessage {
  background-color: $color-white;
  border-radius: 20px;
  padding: 20px;
  margin-bottom: 30px;

  & ul {
    padding: 0;
  }
  & h3 {
    font-family: $font-body;
    font-weight: normal;
  }
}

.formMessage_title {
  margin-bottom: 0;
}

.formMessage .errors {
  color: red;
}

.formGroups {
  display: flex;
  flex-flow: wrap row;
  justify-content: space-between;
}

.formGroup {
  display: block;
  margin-bottom: 20px;
  text-align: left;

  & a {
    color: $color-darker-grey;
  }
}
.formGroup-white {
  margin-bottom: 20px;

  & label {
    color: white;
  }
  & input {
    margin: 10px 0 0 0;
    background-color: $color-white;
  }
}
.formGroup-half {
  width: 100%;
  display: inline-block;
  text-align: left;

  @media (min-width: 768px) {
    width: calc(50% - 10px);
  }
}

.visually-hidden,
.icon__fallback-text {
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
}

.label {
  text-align: left;
  font-weight: normal;
  margin-bottom: 10px;
  font-family: $font-body;
  color: #4c4c4b;
  font-size: 1.4rem;
}

.input {
  background: $color-white;
  border: 1px solid #cccccc;
  display: inline-block;
  font-family: inherit;
  font-size: inherit;
  font-weight: normal;
  outline: none;
  vertical-align: middle;
  width: 100%;
  padding: 0 10px;
  min-width: 100px;
  color: #4c4c4b;
  margin: 10px 0 0;

  &:focus {
    border: 1px solid $color-black;
    box-shadow: none;
    outline: none;
  }
}

.input[type="email"],
.input[type="search"],
.input[type="tel"],
.input[type="password"],
.input[type="text"],
.input[type="number"],
.input[type="date"] {
  height: 40px;
  line-height: 60px;
  -webkit-appearance: none;
}

.input::placeholder {
  /* Chrome, Firefox, Opera, Safari 10.1+ */
  color: grey;
  opacity: 0.5; /* Firefox */
  letter-spacing: 1px;
}

.input:-ms-input-placeholder {
  /* Internet Explorer 10-11 */
  color: $color-black;
}

.input::-ms-input-placeholder {
  /* Microsoft Edge */
  color: $color-black;
}

.input-textarea,
textarea {
  padding: 10px;
  -webkit-appearance: none;
  resize: none;
}

// Custom checkbox
.checkbox {
  width: 100%;
  position: relative;
  display: flex;

  input {
    background: transparent;
    background-color: transparent;
    border: 1px solid $color-black;
    cursor: pointer;
    padding: 0;
    margin: 0;
    position: absolute;
    top: 0;
    left: 0;
    height: 10px;
    width: 10px;
    overflow: hidden;
    outline: none;
    display: flex;
    justify-content: center;
    align-items: center;
    -webkit-appearance: none;

    &:checked {
      background-image: url("https://cdn.shopify.com/s/files/1/0274/8609/3447/files/close-icon_14.svg?v=1602686106");
      color: $color-white;

      &::after {
        text-transform: none;
        letter-spacing: 0;
        font-variant: initial;
        line-height: 1;
      }
    }
  }

  label {
    color: #4c4c4b;
    line-height: normal;
    text-align: left;
    margin-left: 30px;

    &:hover {
      cursor: pointer;
    }

    a {
      color: inherit;
      text-decoration: underline;
    }
  }
}

// Custom select
.select-styled {
  font-family: $font-body;
  padding: 0;
  margin: 0;
  border: 1px solid $color-black;
  min-width: 150px;
  height: 60px;
  line-height: 60px;
  border-radius: 0;
  overflow: hidden;
  background-color: transparent;
  // background: transparent url({{ 'arrowhead-down.svg' | asset_url }}) no-repeat 95% 50%;
  background-size: 15px auto;
  outline: none;

  select {
    color: inherit;
    padding: 0 20px;
    font-family: inherit;
    font-size: inherit;
    width: 100%;
    height: 100%;
    border: none;
    box-shadow: none;
    cursor: pointer;
    background-color: transparent;
    background-image: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    outline: none;

    option {
      color: $color-black;
    }
  }
}
.password-title {
  color: $color-darker-grey;
  font-size: 2rem;
  margin: 20px 0;
}
.password-description {
  font-family: $font-body;
  color: grey;
  font-size: 1.4rem;
}
.signupForm {
