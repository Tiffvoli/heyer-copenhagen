// Responsive table
table {
  margin-bottom: 20px;
  width: 100%;

  a {
    border-bottom: 1px solid currentColor;
  }
}

th {
  font-family: inherit;
  font-style: inherit;
  font-weight: bold;
}

th,
td {
  text-align: left;
  padding: 10px 14px;
}

tbody th,
tfoot th {
  font-weight: normal;
}

// Responsive tables, defined with .responsive-table on table element.
@media (max-width: 768px) {
  .responsiveTable {
    thead {
      display: none;
    }

    th,
    td {
      float: left;
      clear: left;
      width: 100%;
      text-align: right;
      padding: 5px 0;
      border: 0;
      margin: 0;
    }

    th::before,
    td::before {
      content: attr(data-label);
      float: left;
      text-align: center;
      padding-right: 10px;
      font-weight: bold;
    }
  }

  .responsiveTable_row + .responsiveTable_row,
  tfoot > .responsiveTable_row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: 40px;

    &::after {
      content: "";
      display: block;
      position: absolute;
      top: 0;
      left: 40px / 2;
      right: 40px / 2;
      border-bottom: 1px solid $color-black;
    }
  }
}

// Modal
.modal {
  background-color: rgba($color-black, 0.4);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 500;
  opacity: 0;
  visibility: hidden;
  transition: all 0.3s ease-in-out;
}
.overflow-hidden {
  overflow: hidden;
}
.visibility-open {
  opacity: 1;
  visibility: visible;
}
.slide-in {
  transform: translateX(-55rem);
}
.slide-out {
  animation: slide-out 0.5s ease-in-out;
}
.modal h2 {
  font-size: 2.5rem;
}

.modal_container {
  background-color: transparent;
  position: fixed;
  top: 0;
  right: -55rem;
  z-index: 800;
  width: calc(100% - 1rem);
  max-width: 45rem;
  height: 100%;
  overflow-y: scroll;
  -ms-overflow-style: none;
  scrollbar-width: none;
  transition: all 0.3s ease-in-out;
}
.modal_container::-webkit-scrollbar {
  display: none;
}

@keyframes slide-in {
  100% {
    transform: translateX(-45rem);
  }
}
@keyframes slide-out {
  100% {
    transform: translateX(0);
  }
}
.modal-container-wrapper {
  padding: 10rem 3rem;
  float: right;
  width: calc(100% - 5rem);
  height: 100%;
  overflow-y: scroll;
  background-color: $color-white;
  -ms-overflow-style: none;
  scrollbar-width: none;
  word-break: break-word;

  & h3 {
    font-size: 2rem;
    margin: 2rem 0;
    font-family: $font-body;
    font-weight: normal;
  }
  & p {
    padding-bottom: 3rem;
    border-bottom: 0.2px solid #e3e3e3;
  }
  & img {
    width: 100%;
  }
  & table {
    border-spacing: 0;
    border-collapse: collapse;
  }
  & th,
  td {
    border: 0.2px solid #e3e3e3;
    width: 50%;
  }
}
.modal_container-wrapper::-webkit-scrollbar {
  display: none;
}
.modal_close {
  background-color: $color-white !important;
  position: fixed;
  top: 8rem;
  //bottom: 2rem;
  z-index: 2;
  width: 4rem;
  height: 4rem;
  border-radius: 50%;
  border: 0.2px solid #e3e3e3;
  cursor: pointer;
}

.modal_header {
  text-align: center;
  margin-bottom: 25px;

  @media (min-width: 768px) {
    margin-bottom: 75px;
  }
}

.modal_title {
  font-size: 2.8rem;
  font-weight: bold;
  font-stretch: normal;
  font-style: normal;
  line-height: 1.54;
  max-width: unset;

  @media (min-width: 768px) {
    font-size: 4.8rem;
    max-width: 15ch;
    line-height: 1;
  }
}

