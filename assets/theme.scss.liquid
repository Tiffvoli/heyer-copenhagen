// Hover Collection Image
.reveal-collection .hidden-collection {
  display: block !important;
  visibility: visible !important;
}
.product:hover .reveal-collection img {
  opacity: 1;
}
.reveal-collection {
  position: relative;
}
.reveal-collection .hidden-collection {
  position: absolute;
  z-index: -1;
  top: 0;
  width: 100%;
  height: auto;
  opacity: 0;
  -webkit-transition: opacity 0.3s ease-in-out;
  -moz-transition: opacity 0.3s ease-in-out;
  -o-transition: opacity 0.3s ease-in-out;
  transition: opacity 0.3s ease-in-out;
}
.reveal-collection:hover .hidden-collection {
  z-index: 800;
  opacity: 1;
}
.reveal-collection .caption {
  position: absolute;
  top: 0;
  display: table;
  width: 100%;
  height: auto;
  background-color: white; /* fallback for IE8 */
  background-color: rgba(255, 255, 255, 0.7);
  font: 13px/1.6 sans-serif;
  text-transform: uppercase;
  color: #333;
  letter-spacing: 1px;
  text-align: center;
  text-rendering: optimizeLegibility;
}
.reveal-collection .hidden-collection .caption .centered {
  display: table-cell;
  vertical-align: middle;
}

@media (max-width: 480px) {
  .reveal-collection .hidden-collection {
    display: none !important;
  }
}
// Pagination
.pagination {
  display: block;
  text-align: left;
  width: 100%;
  padding-top: 20px;
  margin-top: 40px;
  margin-bottom: 40px;

  @media (min-width: 768px) {
    text-align: center;
  }
}

.pagination_container {
  display: flex;
  justify-content: center;
  align-items: center;
}

.pagination_button {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 32px;
  height: 32px;
  background-color: $color-light-grey;
  color: $color-black;
  border: none;
  border-radius: 50%;
  margin: 0 5px;
  -webkit-appearance: none;
  outline: none;
  cursor: pointer;
  order: 2;

  &:disabled {
    opacity: 0.85;
    cursor: not-allowed;
  }
}

.pagination_button-prev {
  &::before {
    content: "<";
    width: 24px;
    height: 24px;
  }
}

.pagination_button-next {
  &::before {
    content: ">";
    width: 24px;
    height: 24px;
  }
}

.pagination_numbers {
  margin: 0;
  display: block;
  order: 1;
  width: 100%;

  li {
    list-style: none;
    display: inline-block;
  }

  @media (min-width: 768px) {
    margin: 0 35px;
    order: 2;
    width: auto;
  }
}

.pagination_number {
  font-size: 1.8rem;
  color: $color-black;
  opacity: 0.5;
  margin: 0 8.5px;
  transition: opacity 200ms ease-in-out;

  &:hover {
    opacity: 1;
  }

  a {
    font-size: inherit;
    font-weight: 300;
  }
}
.pagination_number-current {
  opacity: 1;

  a {
    cursor: default;
    font-weight: bold;
  }
}
