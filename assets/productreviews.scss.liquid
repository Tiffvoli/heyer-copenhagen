/*------- Stamped Reviews ---------*/
.fa-star,
.fa-star-checked,
.fa-star-half-o,
.fa-star-o,
.stamped-fa-star,
.stamped-fa-star-checked,
.stamped-fa-star-half-o,
.stamped-fa-star-o {
  padding: 0;
  color: rgba(128, 130, 132, 1) !important;
}
.stamped-container {
  font-family: "CalibreRegular", sans-serif !important;
}

div.stamped-container[data-widget-style],
div[data-widget-style] div.stamped-container {
  max-width: 100% !important;
  margin: 30px auto !important;
  border: none !important;
  padding: 0 3% !important;
}
#stamped-main-widget {
  margin: 1em 3% !important;
}
.summary-rating-bar-content {
  background: rgba(128, 130, 132, 1) !important;
}
